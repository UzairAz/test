<odoo>
    <data>
        <record id="inherit_stock_quant_tree_inventory" model="ir.ui.view">
            <field name="name">inherit_stock_quant</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_tree_inventory_editable"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='product_id']" position="after">
                    <field name="warehouse_id"/>
                    <field name="get_warehouse" invisible="1"/>
                </xpath>
            </field>
        </record>
        <record id="inherit_stock_quant_tree" model="ir.ui.view">
            <field name="name">inherit_stock_quant</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_tree_editable"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='product_id']" position="after">
                    <field name="warehouse_id"/>
                    <field name="get_warehouse" invisible="1"/>
                </xpath>
            </field>
        </record>
        <!-- Inherit Search View -->
        <record id="inherit_stock_quant_search_view" model="ir.ui.view">
            <field name="name">group.search.form</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.quant_search_view"/>
            <field name="arch" type="xml">
                <filter name="on_hand" position="after">
                    <filter name="warehouse_id" string='Warehouse' context="{'group_by' :'warehouse_id'}"/>
                </filter>
            </field>
        </record>
    </data>
</odoo>